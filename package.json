{
  "name": "parseit",
  "private": true,
  "version": "1.0.0",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint \"src/**/*.{js,ts,vue}\" --fix",
    "prettier": "prettier --write .",
    "tsc": "vue-tsc --noEmit",
    "dev": "concurrently --kill-others-on-fail \"npm run start:proxy\" \"vite\"",
    "dev-app": "electron-vite dev -w",
    "build": "vue-tsc -b && vite build",
    "build-app": "electron-vite build",
    "start:proxy": "npx cors-proxy",
    "pack:win": "npm run build-app && electron-builder --win",
    "pack:mac": "npm run build-app && electron-builder --mac",
    "pack:linux": "npm run build-app && electron-builder --linux",
    "release": "node release.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@mozilla/readability": "0.5.0",
    "@toast-ui/editor": "^3.2.2",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^3.1.0",
    "chokidar": "^4.0.1",
    "electron-store": "^10.0.0",
    "gui": "github:kirillgalushko/gui#v3.11.0",
    "js-yaml": "^4.1.0",
    "pinia": "2.2.4",
    "sanitize-filename": "^1.6.3",
    "turndown": "^7.2.0",
    "uuid": "9.0.0",
    "vue": "3.5.10"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^22.7.9",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "concurrently": "^9.0.1",
    "cors-proxy": "github:kirillgalushko/cors-proxy",
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^2.3.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^8.0.0",
    "inquirer": "^12.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  }
}
